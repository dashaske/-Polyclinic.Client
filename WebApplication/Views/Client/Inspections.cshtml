@{
    ViewData["Title"] = "Eckeub";
}
<div class="text-center text-info">
    <h1>Обследования</h1>
</div>

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<table class="table">
    <tr>
        <td>Доктор</td>
        <td>Название</td>
        <td>Цена</td>
    </tr>
    @foreach (var  ins in ViewBag.In)
    {
        @foreach (var doctor in ViewBag.Doctor)
        {
            @if (doctor.Id == ins.UserId)
            {
                 <tr>
                     <td>@doctor.FIO</td>
                    <td>@ins.Name</td>
                     <td>@ViewBag.Cena[ins.Id]</td>
                 </tr>
            }
        }
    }
</table>